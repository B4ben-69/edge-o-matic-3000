{
    "displayName": "Bell Ringer",
    "match": {
        "vid": 1,
        "pid": 1,
        "serial": null
    },
    "config": {
        "gpio": {
            "0": {
                "mode": "OUTPUT",
                "level": "LOW"
            }
        }
    },
    "functions": {
        "@high": {
            "setPin": {
                "pin": 0,
                "level": "HIGH"
            }
        },
        "@low": {
            "setPin": {
                "pin": 0,
                "level": "LOW"
            }
        },
        "@pulse": [
            "@high",
            {
                "delay": {
                    "ms": "1000"
                }
            },
            "@low"
        ]
    },
    "events": {
        "orgasmDenial": [
            "@pulse"
        ]
    }
}